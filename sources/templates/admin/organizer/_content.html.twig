    <div id="content" class="table-responsive">
        <table id="dataTable" class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                    <th>Roles</th>
                    <th>Nom</th>
                    <th>Société</th>
                    <th>Est validé</th>
                    <th>Inscription le </th>
                    <th>Détail</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                {% for user in users %}
                <tr>
                    <th>{{user.id}}</th>
                    <th>{{user.email}}</th>
                    <th>{% for role in user.getRoles %}
                            {{role}}
                    {% endfor %}</th>
                    <th>{{user.lastname}}</th>
                    <th>{{user.companyName}}</th>
                    <th>
                        <div class="form-check form-switch">
                            {% set isChecked = (user.isValidate)? "checked" : "" %}
                            <input data-user="{{user.id}}" class="form-check-input" type="checkbox" id="isValidate" {{isChecked}}>
                        </div>
                    </th>
                    <th>{{user.registerAt | date('Y-m-d H:i') }}</th>
                    <th>quelque chose</th>
                    <th>
                        <a href="{{path('organizercrud_edit',{id:user.id})}}">Editer</a>
                        <a href="{{path('organizercrud_delete',{id:user.id})}}">Supprimer</a>
                    </th>
                </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>